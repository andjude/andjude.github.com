  color:black;
  background:#fff;
  text-decoration:underline;
}
a.title {
  background:none;
  padding:0;
}
span.title a {
  background:#fff;
  color:#9c0000;
}
span.title a:hover {
  color:#fff;
  background:#9c0000;
  text-decoration:underline;
}
ul.home-posts li {
  line-height:1.8em;
}

#outer-wrapper {
  max-width:960px; 
  min-width:960px;
  margin: 1em auto;
  margin-top:90px;
}

#header-wrapper {
  padding:0;
  margin:0;
  margin-top:-21px;
  height:84px;
}

#main-wrapper {
  width:474px;
  padding:6px;
  padding-top:0px;
  float:left;
}

#sidebar-wrapper {
  width:474px;
  float:right;
}

.side-single-column {
  float:left;
  padding:6px;
  width:224px;
}

.side-double-column {
  padding:6px;
  float:left;
  width:474px;
}

span { 
  margin:0;
  padding:0;
}

span.reverse {
  color: #FFF;
  background:#44e;  
}
div.section {
  float:left;
  display:block;
}
div.button {
  float:right;
  margin:5px;
}
#footer-wrapper {
  background:#444 url(http://andjude.github.com/images/background.png);
  margin-bottom:-30px;
}
div.footer {
  max-width:960px; 
  min-width:960px;
  margin: 1em auto;
}
div.footer .links a {
  color:white;
  text-decoration:none;
}
div.footer .links a:hover {
  color:black;
  text-decoration:underline;
}
div.footer ul { padding:0; }
div.footer ul.links { margin: 0; top: 0; height: 36px; float: left; font-size:16px; padding-top:30px;}
div.footer ul.links li { display: block; line-height: 36px; border-left:1px dotted silver; padding-left:15px;}
div.footer ul.links.first li { border-left:none !important}

div.tweet { border-left: 1px dotted silver; padding:0 16px; height: 216px; text-align:right;}
div.tweet ul { margin: 0; top: 0; height: 36px; float: left; font-size:16px, text-align:right;}
div.tweet ul li { display: block; line-height: 22px; padding:6px;}
div.tweet img {vertical-align:middle;margin-right:6px;}
div.tweet .tweet_even a { color: white; font-weight:bold; }
div.tweet .tweet_odd a { color: red; font-weight:bold; }
div.tweet .tweet_even { background:#ff000e; color:white; }
div.tweet .tweet_odd { background:white; color:#ff000e; }
div.tweet .tweet_avatar img { float:left; }

div.post-footer {
  color:gray;
  font-size:small;
}

div.home-listing {
  width:100%;
}

ul.home-posts { 
  padding:0;
  border-left:1px solid #333;
  list-style-type:none;
}

ul.home-posts li {
  min-height:60px;
  padding:6px;
}

ul.home-posts li:nth-child(odd) { 
  background: rgba(96, 96, 96, 0.1); 
}

ul.home-posts .title {
  color:white;
}

ul.home-posts .date {
  color:gray;
}

ul.home-posts a {
  font-weight:bold;
  text-decoration:none;
}

h2 { font-size:1.8em }

#main-wrapper p { font-size:1.4em }
#main-wrapper p+p { font-size:1em }

a.button {
  background:url("http://andjude.github.com/images/button.png") no-repeat scroll 0 0 transparent;
  border:medium none;
  color:white;
  font-size:1em;
  height:43px;
  width:104px;
  display:block;
  float:left;
  text-align:center;
  padding-top:10px;
  margin-right:10px;
}    
a.button:hover {
  text-decoration:none;
}


