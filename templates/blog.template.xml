  display:none !important;
}

#navbar-iframe {
  display: none !important;
}

body {
  margin:0;
  padding:0;
  color:#bbb;
  font-family: HelveticaNeue-Light,Helvetica,Arial,sans-serif; 
  line-height: 1.5;
}

a {
  color:white;
  text-decoration:none;
  background:#9c0000;
  padding:3px;
}
